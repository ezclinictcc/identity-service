{
  "env": {
    "commonjs": false,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "interface",
        "format": ["PascalCase"],
        "custom": {
          "regex": "^I[A-Z]",
          "match": true
        }
      }
    ],
    "quotes": ["error", "double"],
    "max-len": "off",
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "linebreak-style": ["off", "windows"],
    "no-console": "off",
    "class-methods-use-this": "warn",
    "no-use-before-define": "warn",
    "no-unused-vars": "warn",
    "indent": "warn",
    "comma-dangle": "warn",
    "brace-style": "warn",
    "object-curly-newline": "warn",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "no-shadow": "off",
    "operator-linebreak": "off"
  }
}
