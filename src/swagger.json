      },
      "post": {
        "tags": ["User"],
        "summary": "Create",
        "description": "Create a new user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserEntity"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User Created"
          },
          "500": {
            "description": "Unexpected error"
          }
        }
      },
      "put": {
        "tags": ["User"],
        "summary": "Update",
        "description": "Update  an existing user",
        "requestBody": {
