            "description": "User city"
          },
          {
            "in": "query",
            "name": "district",
            "example": "Bairro X, Rua Y",
            "schema": {
              "type": "string"
            },
            "description": "User district"
          },
          {
            "in": "query",
            "name": "number",
            "example": "12345",
            "schema": {
              "type": "number"
            },
            "description": "User street number"
          },
          {
            "in": "query",
            "name": "cep",
            "example": "13040-700",
            "schema": {
              "type": "string"
            },
            "description": "User cep"
